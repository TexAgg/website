@using System.ServiceModel.Syndication
@model SyndicationFeed

@section AdditionalMeta
{
    <meta name="robots" content="noindex,nofollow"/>
}

<div class="container panel panel-default">
	<div class="panel-heading">
		<h2 class="page-title">@ViewContext.RouteData.Values["Controller"]</h2>
	</div>
	<div class="panel-body">
		<ul class="list-group">
			@foreach(var i in @Model.Items)
			{
				<li class="list-group-item">@Html.ActionLink(@i.Title.Text,"Post", "Blog", new{title=@i.Title.Text}, null)<small style="float:right;">@i.PublishDate.ToString("d")</small></li>
			}
		</ul>
	</div>
</div>
