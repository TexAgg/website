@model website.Models.Skills

<div id='skills' class="container panel panel-default">
	<div class="panel-heading">
		<h2>@ViewBag.Title</h2>
	</div>
	<ul class="skills-list"><u>Proficient in:</u>
	@foreach (var skill in Model.Proficient)
	{
		<li>@skill</li>
	}
	</ul>
	<ul class="skills-list"><u>Familiar with:</u>
	@foreach (var skill in Model.Familiar)
	{
		<li>@skill</li>
	}
	</ul>
</div>
